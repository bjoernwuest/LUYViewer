{
  "name": "deno-browser-app",
  "version": "1.0.0",
  "exports": "./main.ts",
  "tasks": {
    "start": "deno run --allow-net --allow-read --allow-write=./data main.ts",
    "dev": "deno run --allow-net --allow-read --allow-write=./data --watch main.ts",
    "compile": "deno compile --allow-net --allow-read --allow-write=./data --target x86_64-pc-windows-msvc --output LUYViewer.exe main.ts"
  },
  "imports": {
    "@std/path": "jsr:@std/path@^1.0.0",
    "@std/jsonc": "jsr:@std/jsonc@^1.0.0"
  }
}
